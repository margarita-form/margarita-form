{
  "name": "margarita-form",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Modern form library made with TypeScript",
  "keywords": [
    "Form",
    "Forms",
    "Form alternative",
    "Form library",
    "Reactive form",
    "React form"
  ],
  "scripts": {
    "dev": "nx run baking-oven:serve",
    "dev:angular-demos": "nx run angular-demos:serve",
    "gen:lib": "./generate-lib.sh",
    "test:core": "nx run margarita-form:lint && nx run margarita-form:test && npm run test:core-types && npm run build:core",
    "test:core-types": "tsd libs/margarita-form --files src/typings/*.test-d.ts",
    "test:react": "nx run margarita-form-react:lint && npm run build:react",
    "test:angular": "nx run margarita-form-angular:lint && npm run build:angular && npm run build:angular-demos",
    "test:all": "npm run test:core && npm run test:react && npm run test:angular",
    "pretty": "prettier --write \"libs/**/*.{js,jsx,ts,tsx,json,md}\"",
    "build:core": "nx run margarita-form:build",
    "build:react": "nx run margarita-form-react:build",
    "build:angular": "nx run margarita-form-angular:build",
    "build:angular-demos": "nx run angular-demos:build",
    "build:all": "nx run-many -t build -p margarita-form-core margarita-form-react margarita-form-angular --skipNxCache --nxBail",
    "publish:core": "cd dist/libs/margarita-form && npm publish",
    "publish:react": "cd dist/libs/margarita-form-react && npm publish",
    "publish:angular": "cd dist/libs/margarita-form-angular && npm publish",
    "publish:all-dev": "npm run test:all && npm run create-docs && lerna version --force-publish -m \"chore(release-dev): version %v\" && rm -rf dist && npm run build:all && node ./scripts/add-readme.mjs && node ./scripts/update-dependencies.mjs && npm run publish:core --tag dev && npm run publish:react --tag dev && npm run publish:angular --tag dev",
    "publish:all": "npm run test:all && npm run create-docs && lerna version --force-publish -m \"chore(release): version %v\" && npm run build:all && node ./scripts/add-readme.mjs && node ./scripts/update-dependencies.mjs && npm run publish:core && npm run publish:react && npm run publish:angular",
    "create-docs": "typedoc",
    "serve-docs": "cd doc && npx http-server -p 8080"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular-devkit/core": "^17.0.3",
    "@angular-devkit/schematics": "^17.0.3",
    "@angular-eslint/eslint-plugin": "^17.0.1",
    "@angular-eslint/eslint-plugin-template": "^17.0.1",
    "@angular-eslint/template-parser": "^17.0.1",
    "@angular/cli": "^17.0.0",
    "@angular/compiler-cli": "^17.0.4",
    "@angular/language-service": "^17.0.4",
    "@nx/angular": "17.2.8",
    "@nx/eslint": "17.2.8",
    "@nx/eslint-plugin": "17.2.8",
    "@nx/js": "17.2.8",
    "@nx/node": "17.2.8",
    "@nx/react": "17.2.8",
    "@nx/vite": "17.2.8",
    "@nx/workspace": "17.2.8",
    "@schematics/angular": "^17.0.3",
    "@swc-node/register": "~1.6.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.90",
    "@testing-library/react": "13.4.0",
    "@types/child-process-promise": "^2.2.2",
    "@types/inquirer": "^9.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "18.11.9",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.9",
    "@types/react-is": "18.2.2",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-c8": "0.32.4",
    "@vitest/ui": "0.34.7",
    "babel-plugin-styled-components": "1.10.7",
    "child-process-promise": "^2.2.1",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "inquirer": "^9.1.5",
    "jsdom": "22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "ng-packagr": "17.0.2",
    "nx": "17.2.8",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "tsd": "^0.29.0",
    "typedoc": "^0.25.3",
    "typescript": "5.2.2",
    "vite": "5.0.7",
    "vite-plugin-dts": "2.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.6",
    "yup": "^1.3.2",
    "zod": "^3.22.4"
  },
  "dependencies": {
    "@angular/animations": "17.0.4",
    "@angular/common": "17.0.4",
    "@angular/compiler": "17.0.4",
    "@angular/core": "17.0.4",
    "@angular/forms": "17.0.4",
    "@angular/platform-browser": "17.0.4",
    "@angular/platform-browser-dynamic": "17.0.4",
    "@angular/router": "17.0.4",
    "@swc/helpers": "0.5.2",
    "lerna": "^7.1.1",
    "lodash.get": "^4.4.2",
    "nanoid": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rxjs": "^7.8.1",
    "styled-components": "5.3.6",
    "tslib": "^2.3.0",
    "zone.js": "0.14.2"
  },
  "workspaces": [
    "packages/*"
  ]
}
