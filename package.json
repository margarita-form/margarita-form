{
  "name": "margarita-form",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Modern form library made with TypeScript",
  "keywords": [
    "Form",
    "Forms",
    "Form alternative",
    "Form library",
    "Reactive form",
    "React form"
  ],
  "scripts": {
    "dev": "nx run baking-oven:serve",
    "gen:lib": "./generate-lib.sh",
    "test:core": "nx run margarita-form:test",
    "build:core": "nx run margarita-form:build",
    "build:react": "nx run margarita-form-react:build",
    "build:all": "npm run build:core && npm run build:react",
    "publish:core": "cd dist/libs/margarita-form && npm publish",
    "publish:react": "cd dist/libs/margarita-form-react && npm publish",
    "publish:all": "lerna version --force-publish -m \"chore(release): version %v\" && node ./scripts/update-dependencies.mjs && npm run build:all && node ./scripts/add-readme.mjs && npm run publish:core && npm run publish:react"
  },
  "private": true,
  "devDependencies": {
    "@nrwl/eslint-plugin-nx": "15.7.1",
    "@nrwl/js": "^15.7.1",
    "@nrwl/linter": "15.7.1",
    "@nrwl/node": "^15.7.1",
    "@nrwl/react": "^15.7.1",
    "@nrwl/vite": "15.7.1",
    "@nrwl/workspace": "15.7.1",
    "@swc/cli": "~0.1.55",
    "@swc/core": "^1.2.173",
    "@testing-library/react": "13.4.0",
    "@types/child-process-promise": "^2.2.2",
    "@types/inquirer": "^9.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-is": "17.0.3",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "babel-plugin-styled-components": "1.10.7",
    "child-process-promise": "^2.2.1",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "inquirer": "^9.1.5",
    "jsdom": "~20.0.3",
    "nx": "15.7.1",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "typescript": "~4.8.2",
    "vite": "^4.0.1",
    "vite-plugin-dts": "~1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8"
  },
  "dependencies": {
    "@margarita-form/core": "^0.0.1",
    "@swc/helpers": "~0.4.11",
    "lerna": "^5.4.2",
    "lodash.get": "^4.4.2",
    "nanoid": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "18.2.0",
    "rxjs": "^7.8.0",
    "styled-components": "5.3.6"
  },
  "workspaces": [
    "packages/*"
  ]
}
